<Window x:Class="ViewLocatorTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ViewLocatorTest"
        mc:Ignorable="d"
        
        
        DataContext="{local:Locator Test}"
        
        Title="MainWindow" Height="350" Width="525">
    <StackPanel >
        <TextBox Text="{Binding Title}" FontSize="20" />
        <TextBox Text="{Binding Subtitle}" FontSize="20" />
        <TextBox Text="{Binding Message}" FontSize="20" />
    </StackPanel>
</Window>
